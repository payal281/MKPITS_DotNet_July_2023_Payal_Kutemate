\par \hich\af1\dbch\af31505\loch\f1 p.inset \{border-style: inset; \} 
\par \hich\af1\dbch\af31505\loch\f1 p.outset \{border-style: outset; \} 
\par \hich\af1\dbch\af31505\loch\f1 p.ridge \{border-style: ridge; \} 
\par \hich\af1\dbch\af31505\loch\f1 p.hidden \{border-style: hidden; \}
